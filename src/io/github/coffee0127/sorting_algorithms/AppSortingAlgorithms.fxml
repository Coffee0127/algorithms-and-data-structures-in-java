<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="io.github.coffee0127.sorting_algorithms.AppSortingAlgorithms">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <Label layoutX="14.0" layoutY="14.0" text="Algorithms"/>
    <Label layoutX="194.0" layoutY="14.0" text="Results"/>
    <Button layoutX="14.0" layoutY="39.0" mnemonicParsing="false" text="Selection Sort" prefHeight="25.0" prefWidth="120.0" onAction="#selectionSort"/>
    <Button layoutX="14.0" layoutY="69.0" mnemonicParsing="false" text="Bubble Sort" prefHeight="25.0" prefWidth="120.0" onAction="#bubbleSort"/>
    <Button layoutX="14.0" layoutY="99.0" mnemonicParsing="false" text="Insertion Sort" prefHeight="25.0" prefWidth="120.0" onAction="#insertionSort"/>
    <Button layoutX="14.0" layoutY="129.0" mnemonicParsing="false" text="Merge Sort" prefHeight="25.0" prefWidth="120.0" onAction="#mergeSort"/>
    <Button layoutX="14.0" layoutY="159.0" mnemonicParsing="false" text="Quick Sort" prefHeight="25.0" prefWidth="120.0" onAction="#quickSort"/>
    <Button layoutX="14.0" layoutY="189.0" mnemonicParsing="false" text="New Arrays" prefHeight="25.0" prefWidth="120.0" onAction="#newArrays"/>
    <Button layoutX="14.0" layoutY="219.0" mnemonicParsing="false" text="All Algorithms" prefHeight="25.0" prefWidth="120.0" onAction="#allSort"/>
    <TextArea fx:id="textArea" layoutX="182.0" layoutY="39.0" prefHeight="205.0" prefWidth="200.0"/>
</AnchorPane>
