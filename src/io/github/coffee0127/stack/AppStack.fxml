<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="io.github.coffee0127.stack.AppStack">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <Label layoutX="14.0" layoutY="14.0" text="Stack With Array"/>
    <Button layoutX="14.0" layoutY="38.0" mnemonicParsing="false" onAction="#pushElement" prefWidth="80.0" text="Push"/>
    <Button layoutX="14.0" layoutY="78.0" mnemonicParsing="false" onAction="#popElement" prefWidth="80.0" text="Pop"/>
    <Button layoutX="14.0" layoutY="118.0" mnemonicParsing="false" onAction="#peekElement" prefWidth="80.0" text="Peek"/>
    <TextArea fx:id="textArea" layoutX="104.0" layoutY="38.0" prefHeight="200.0" prefWidth="200.0"/>
</AnchorPane>
