<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.coffee0127.linkedlist.AppLinkedList">
    <TextField fx:id="insertElement" layoutX="14.0" layoutY="38.0" prefWidth="292.0">
